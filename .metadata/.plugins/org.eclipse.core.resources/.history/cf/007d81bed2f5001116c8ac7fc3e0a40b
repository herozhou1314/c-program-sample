#include <string.h>
#include <stdlib.h>
void util_print(char *print_str)

{
	printf("printString-->> %s\n", print_str);
}
void util_print_int(int i)

{
	printf("printInt-->> %d\n", i);
}

void util_print_float(float f)

{
	printf("printFloat-->> %f\n", f);
}
/**
 *  用C语言实现字符串替换功能
 * char *source,
 *    原字符串
*char *sub,
 *          被代替的字符串
 *char *rep
 *   代替的字符串啊
 */

